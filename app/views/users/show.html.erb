<h2><%=@user.name%></h2>
<p>
    <label>My Cars:</label><br>
    <%@user.cars.each do |car|%>
        <%= link_to car.make + " " + car.model, car_path(car)%><br>
    <% end %>
</p>
<p>
    <label>All Users on My Account:</label><br>
    <%co_users = User.all.where(family_id: @user.family_id).select {
        |user| user != @user
    }%>
    <%co_users.each do |user|%>
        <%=user.name%><br>
    <% end %>
</p>
<div>
<%= link_to "Add New Car to Account", new_car_path%><br>
<%= link_to "Add New/Existing User to Account", edit_family_path(@user.family_id)%><br>
</div>
<div>
<%=link_to "Edit User", edit_user_path(@user)%><br>
<%= link_to 'Delete User', user_path(@user), method: :delete, data: { confirm: 'Are you sure?' } %> 
</div>